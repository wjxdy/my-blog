<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xulei.myblogbackend.mapper.ArticleTagMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xulei.myblogbackend.entity.ArticleTag">
        <id column="article_tag_id" property="articleTagId" />
        <result column="article_tag_name" property="articleTagName" />
        <result column="artcle_tag_add_time" property="artcleTagAddTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        article_tag_id, article_tag_name, artcle_tag_add_time
    </sql>
    <select id="getTagList" resultType="com.xulei.myblogbackend.vo.ArticleTagVo">
        select article_tag_id, article_tag_name from article_tag
    </select>

</mapper>
